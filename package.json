    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.38.0",
    "eslint-config-next": "^15.0.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "postcss": "^8.4.0",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0"
  }
